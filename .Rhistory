estimate_mdiff_ind_contrast(
means = means,
sds = sds,
ns = ns,
contrast = contrast,
grouping_variable_levels = grouping_variable_levels,
outcome_variable_name = "% time near target",
grouping_variable_name = "Condition",
conf_level = 0.99,
assume_equal_variance = TRUE
)
